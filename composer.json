{
    "name": "wax/wax",
    "description": "PHP Wax v5",
    "keywords": ["microframework"],
    "homepage": "http://php-wax.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Riley",
            "email": "ross@oneblackbear.com"
        },
        {
            "name": "Charles Marshall",
            "email": "charles@oneblackbear.com"
        },
