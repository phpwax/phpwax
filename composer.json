            "email": "sheldon@oneblackbear.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "pimple/pimple": "1.*",
        "symfony/event-dispatcher": "2.1.*",
        "symfony/http-foundation": "2.1.*",
        "symfony/http-kernel": "2.1.*",
        "symfony/routing": "2.1.*",
        "symfony/class-loader": "2.1.*"
    },
    "require-dev": {
        "symfony/security": "2.1.*",
        "symfony/config": "2.1.*",
        "symfony/locale": "2.1.*",
        "symfony/form": "2.1.*",
        "symfony/browser-kit": "2.1.*",
        "symfony/finder": "2.1.*",
        "symfony/validator": "2.1.*"
    },
    "autoload": {
        "psr-0": { "Wax": "src/" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
